akka {
 debug {  
              receive = on 
              autoreceive = on
              lifecycle = on
              event-stream = on
              unhandled = on
        }
	loglevel=INFO
    actor.provider = cluster
	log-config-on-start = on       
    remote {
        dot-netty.tcp {
			maximum-frame-size = 16000000b
            port = @PORT
			public-hostname = @PUBHOSTNAME
            hostname = @HOSTNAME
		}
    }
    cluster {
        seed-nodes = [@SEEDHOSTS]
    }	
}

